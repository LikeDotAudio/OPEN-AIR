{
  "nab": {
    "type": "OcaBlock",
    "description": "NAB Marker Monitoring",
    "fields": {
      "NAB_all_marker_settings": {
        "type": "OcaBlock",
        "description": "Marker Amplitude and Frequency Readbacks",
        "fields": {
          "Execute Command": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "trigger": false,
            "message": ":CALCulate:MARKer1:Y?;:CALCulate:MARKer2:Y?;:CALCulate:MARKer3:Y?;:CALCulate:MARKer4:Y?;:CALCulate:MARKer5:Y?;:CALCulate:MARKer6:Y?;:CALCulate:MARKer1:X?;:CALCulate:MARKer2:X?;:CALCulate:MARKer3:X?;:CALCulate:MARKer4:X?;:CALCulate:MARKer5:X?;:CALCulate:MARKer6:X?",
            "layout": { "height": 30, "sticky": "" }
          },
          "Outputs": {
            "type": "OcaBlock",
            "description": "Marker Values",
            "fields": {
              "Marker_1": {
                "value": "Marker 1 Value in dBm",
                "type": "_GuiValue"
              },
              "Marker_2": {
                "value": "Marker 2 Value in dBm",
                "type": "_GuiValue"
              },
              "Marker_3": {
                "value": "Marker 3 Value in dBm",
                "type": "_GuiValue"
              },
              "Marker_4": {
                "value": "Marker 4 Value in dBm",
                "type": "_GuiValue"
              },
              "Marker_5": {
                "value": "Marker 5 Value in dBm",
                "type": "_GuiValue"
              },
              "Marker_6": {
                "value": "Marker 6 Value in dBm",
                "type": "_GuiValue"
              },
              "Marker_1_X": {
                "value": "Marker 1 Frequency in Hz",
                "type": "_GuiValue"
              },
              "Marker_2_X": {
                "value": "Marker 2 Frequency in Hz",
                "type": "_GuiValue"
              },
              "Marker_3_X": {
                "value": "Marker 3 Frequency in Hz",
                "type": "_GuiValue"
              },
              "Marker_4_X": {
                "value": "Marker 4 Frequency in Hz",
                "type": "_GuiValue"
              },
              "Marker_5_X": {
                "value": "Marker 5 Frequency in Hz",
                "type": "_GuiValue"
              },
              "Marker_6_X": {
                "value": "Marker 6 Frequency in Hz",
                "type": "_GuiValue"
              }
            }
          }
        }
      }
    }
  },
  "set": {
    "type": "OcaBlock",
    "description": "Marker Command Sets",
    "fields": {
      "Set_Marker_State_ON": {
        "type": "OcaBlock",
        "description": "Enable Marker",
        "fields": {
          "Execute Command": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "trigger": false,
            "message": ":CALCulate:MARKer<marker_number>:STATe ON",
            "layout": { "height": 30, "sticky": "" }
          },
          "Input": {
            "type": "OcaBlock",
            "fields": {
              "marker_number": {
                "value": "1",
                "type": "_GuiValue"
              }
            }
          }
        }
      },
      "Set_Marker_State_OFF": {
        "type": "OcaBlock",
        "description": "Disable Marker",
        "fields": {
          "Execute Command": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "trigger": false,
            "message": ":CALCulate:MARKer<marker_number>:STATe OFF",
            "layout": { "height": 30, "sticky": "" }
          },
          "Input": {
            "type": "OcaBlock",
            "fields": {
              "marker_number": {
                "value": "1",
                "type": "_GuiValue"
              }
            }
          }
        }
      },
      "Set_Marker_X_position": {
        "type": "OcaBlock",
        "description": "Set Marker Frequency",
        "fields": {
          "Execute Command": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "trigger": false,
            "message": ":CALCulate:MARKer<marker_number>:X <x_value>",
            "layout": { "height": 30, "sticky": "" }
          },
          "Input": {
            "type": "OcaBlock",
            "fields": {
              "marker_number": {
                "value": "1",
                "type": "_GuiValue"
              },
              "x_value": {
                "value": "123456789",
                "type": "_GuiValue"
              }
            }
          }
        }
      }
    }
  }
}