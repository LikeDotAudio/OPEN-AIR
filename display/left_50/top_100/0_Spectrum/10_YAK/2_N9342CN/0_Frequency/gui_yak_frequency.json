        "fields": {
          "Execute Command": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "trigger": false,
            "message": ":FREQuency:STARt <start_freq>;:FREQuency:STOP <stop_freq>",
            "layout": { "height": 30, "sticky": "" }
          },
          "Input": {
            "type": "OcaBlock",
            "fields": {
              "start_freq": {
                "value": "12345678",
