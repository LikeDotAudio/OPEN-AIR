{
  "NAB_Settings": {
    "type": "OcaBlock",
    "description": "NAB Frequency Monitoring",
    "fields": {
      "NAB_Frequency_settings": {
        "type": "OcaBlock",
        "description": "Primary Frequency Monitoring",
        "fields": {
          "Execute Command": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "trigger": false,
            "message": ":FREQuency:STARt?;:FREQuency:STOP?;:FREQuency:CENTer?;:FREQuency:SPAN?",
            "layout": { "height": 30, "sticky": "" }
          },
          "Outputs": {
            "type": "OcaBlock",
            "fields": {
              "start_freq": {
                "value": "The current start frequency in Hz.",
                "type": "_GuiValue"
              },
              "stop_freq": {
                "value": "The current stop frequency in Hz.",
                "type": "_GuiValue"
              },
              "center_freq": {
                "value": "The current center frequency in Hz.",
                "type": "_GuiValue"
              },
              "span_freq": {
                "value": "The current span frequency in Hz.",
                "type": "_GuiValue"
              }
            }
          }
        }
      }
    }
  },
  "RIG_Settings": {
    "type": "OcaBlock",
