{
  "nab": {
    "type": "OcaBlock",
    "description": "Oscilloscope Measurements & Status",
    "fields": {

      "System_Health": {
        "type": "OcaBlock",
        "description": "Errors and ID",
        "fields": {
          "Read_IDN": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "*IDN?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "idn_string": { "value": "Device ID", "type": "_GuiValue", "label": "ID String", "units": "" }
                }
              }
            }
          },
          "Read_Error": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":SYSTem:ERRor?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "error_msg": { "value": "Error Message", "type": "_GuiValue", "label": "Error Message", "units": "" }
                }
              }
            }
          }
        }
      }
    }
  },

   "Do": {
    "type": "OcaBlock",
    "description": "Actions and Toggles",
    "fields": {
      "Run_Control": {
        "type": "OcaBlock",
        "fields": {
          "Do_Run": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "RUN",
            "Execute Command": { "message": ":RUN" }
          },
          "Do_Stop": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "STOP",
            "Execute Command": { "message": ":STOP" }
          },
          "Do_Single": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "SINGLE",
            "Execute Command": { "message": ":SINGle" }
          },
          "Do_Autoscale": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "AUTOSCALE",
            "Execute Command": { "message": ":AUToscale" }
          }
        }
      },
      "Display_Control": {
        "type": "OcaBlock",
        "fields": {
          "Do_Clear_Display": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "Clear Display",
            "Execute Command": { "message": ":DISPlay:CLEar" }
          }
        }
      },
      "System_Actions": {
        "type": "OcaBlock",
        "fields": {
          "Do_Clear_Status": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "CLS",
            "Execute Command": { "message": "*CLS" }
          },
          "Do_Reset": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "Reset (*RST)",
            "Execute Command": { "message": "*RST" }
          }
        }
      }
    }
  }
}