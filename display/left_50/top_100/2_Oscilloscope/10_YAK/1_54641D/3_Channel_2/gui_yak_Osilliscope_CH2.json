{
  "nab": {
    "type": "OcaBlock",
    "description": "Oscilloscope Measurements & Status",
    "fields": {
      
      "Read_Measurements_CH2": {
        "type": "OcaBlock",
        "description": "Channel 2 Measurements",
        "fields": {
          "Get_Voltage_Stats_CH2": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":MEASure:SOURce <source>;:MEASure:VPP?;:MEASure:VRMS?;:MEASure:VMAX?;:MEASure:VMIN?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "source": { "value": "CHANnel2", "type": "_Value",
 "description": "CHAN1, CHAN2, MATH" }
                }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "vpp": { "value": "Vpp Value", "type": "_GuiValue" },
                  "vrms": { "value": "Vrms Value", "type": "_GuiValue" },
                  "vmax": { "value": "Vmax Value", "type": "_GuiValue" },
                  "vmin": { "value": "Vmin Value", "type": "_GuiValue" }
                }
              }
            }
          },
          "Get_Time_Stats_CH2": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":MEASure:SOURce <source>;:MEASure:FREQuency?;:MEASure:PERiod?;:MEASure:DUTYcycle?;:MEASure:RISetime?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "source": { "value": "CHANnel2", "type": "_GuiValue" }
                }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "frequency": { "value": "Freq Value", "type": "_GuiValue" },
                  "period": { "value": "Period Value", "type": "_GuiValue" },
                  "duty_cycle": { "value": "Duty Cycle Value", "type": "_GuiValue" },
                  "rise_time": { "value": "Rise Time Value", "type": "_GuiValue" }
                }
              }
            }
          }
        }
      }
      
    }
  },
  
  "set": {
    "type": "OcaBlock",
    "description": "Channel Settings",
    "fields": {

      "Configure_Channel_2": {
        "type": "OcaBlock",
        "description": "Vertical Setup CH2",
        "fields": {
          "CH2_Scale": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:SCALe <scale>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "scale": { "value": "1.0", "type": "_GuiValue" } }
              }
            }
          },
          "CH2_Offset": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:OFFSet <offset>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "offset": { "value": "0.0", "type": "_GuiValue" } }
              }
            }
          },
          "CH2_Coupling": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:COUPling <coupling>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "coupling": { "value": "DC", "type": "_GuiValue" } }
              }
            }
          },
          "CH2_Probe": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:PROBe <attenuation>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "attenuation": { "value": "10", "type": "_GuiValue" } }
              }
            }
          },
          "CH2_Label": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:LABel <string>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "string": { "value": "\"SIG2\"", "type": "_GuiValue" } }
              }
            }
          },
          "CH2_BWLimit": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:BWLimit <state>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "state": { "value": "ON", "type": "_GuiValue" } }
              }
            }
          },
          "CH2_Invert": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel2:INVert <state>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "state": { "value": "OFF", "type": "_GuiValue" } }
              }
            }
          }
        }
      }
    }
  },
  "Do": {
    "type": "OcaBlock",
    "description": "Actions and Toggles",
    "fields": {
      
      "Display_Control": {
        "type": "OcaBlock",
        "fields": {
          
          "Do_CH2_ON": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "CH2 ON",
            "Execute Command": { "message": ":CHANnel2:DISPlay ON" }
          },
          "Do_CH2_OFF": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "CH2 OFF",
            "Execute Command": { "message": ":CHANnel2:DISPlay OFF" }
          }
        }
      }
    }
  }
}