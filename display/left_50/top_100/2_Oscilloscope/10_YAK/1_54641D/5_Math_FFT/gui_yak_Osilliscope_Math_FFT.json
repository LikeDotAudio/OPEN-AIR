  "nab": {
    "type": "OcaBlock",
    "description": "Math Measurements",
    "fields": {
      "Read_Math_Stats": {
        "type": "OcaBlock",
        "description": "Read statistics for the active Math function",
        "fields": {
          "Get_Math_Statistics": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":MEASure:SOURce FUNCtion;:MEASure:VPP?;:MEASure:VMAX?;:MEASure:VMIN?;:MEASure:VAVerage?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "description": "Measured Math Statistics",
                "fields": {
                  "math_vpp": { "value": "Math Vpp", "type": "_GuiValue", "label": "Vpp", "units": "V" },
                  "math_vmax": { "value": "Math Vmax", "type": "_GuiValue", "label": "Vmax", "units": "V" },
                  "math_vmin": { "value": "Math Vmin", "type": "_GuiValue", "label": "Vmin", "units": "V" },
                 "math_avg": { "value": "Math Avg", "type": "_GuiValue", "label": "Avg", "units": "V" }
                }
              }
            }
          }
        }
      }
    }
  },
