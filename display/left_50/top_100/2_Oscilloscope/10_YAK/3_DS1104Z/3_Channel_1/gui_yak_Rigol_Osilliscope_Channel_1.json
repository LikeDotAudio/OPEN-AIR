{
  "nab": {
    "type": "OcaBlock",
    "description": "Rigol Measurements",
    "fields": {
      "Measurements_CH1": {
        "type": "OcaBlock",
        "description": "Channel 1 Stats",
        "fields": {
          "Get_Stats_CH1": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":MEASure:VPP? CHANnel1;:MEASure:VRMS? CHANnel1;:MEASure:FREQuency? CHANnel1;:MEASure:PDUTy? CHANnel1",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "Vpp": {
                    "label": "Vpp (Volts)",
                    "value": "The peak-to-peak voltage.",
                    "type": "_GuiValue",
                    "units": "Volts"
                  },
                  "Vrms": {
                    "label": "Vrms (Volts)",
                    "value": "The root mean square voltage.",
                    "type": "_GuiValue",
                    "units": "Volts"
                  },
                  "Freq": {
                    "label": "Frequency (Hz)",
                    "value": "The signal frequency.",
                    "type": "_GuiValue",
                    "units": "Hz"
                  },
                  "Duty": {
                    "label": "Duty Cycle (%)",
                    "value": "The duty cycle percentage.",
                    "type": "_GuiValue",
                    "units": "%"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "rig": {
    "type": "OcaBlock",
    "description": "Scope Configuration",
    "fields": {
      "Config_CH1": {
        "type": "OcaBlock",
        "description": "Channel 1 Vertical",
        "fields": {
          "CH1_Scale": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel1:SCALe <scale>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "scale": {
                    "label": "Scale (Volts/Div)",
                    "value": "The current vertical scale.",
                    "type": "_GuiValue",
                    "units": "Volts/Div"
                  }
                }
              }
            }
          },
          "CH1_Offset": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel1:OFFSet <offset>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "offset": {
                    "label": "Offset (Volts)",
                    "value": "The current vertical offset.",
                    "type": "_GuiValue",
                    "units": "Volts"
                  }
                }
              }
            }
          },
          "CH1_Coupling": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel1:COUPling <coupling>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "coupling": {
                    "label": "Coupling",
                    "value": "The current coupling (AC, DC, GND).",
                    "type": "_GuiValue",
                    "units": ""
                  }
                }
              }
            }
          },
          "CH1_Probe": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel1:PROBe <atten>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "atten": {
                    "label": "Probe Attenuation",
                    "value": "The current probe attenuation factor.",
                    "type": "_GuiValue",
                    "units": "X"
                  }
                }
              }
            }
          },
          "CH1_BWLimit": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":CHANnel1:BWLimit <limit>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "limit": {
                    "label": "Bandwidth Limit",
                    "value": "The current bandwidth limit (20M or OFF).",
                    "type": "_GuiValue",
                    "units": ""
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "Do": {
    "type": "OcaBlock",
    "description": "Actions and Toggles",
    "fields": {
      "Run_Control": {
        "type": "OcaBlock",
        "fields": {
          "Run": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "RUN",
            "Execute Command": { "message": ":RUN" }
          },
          "Stop": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "STOP",
            "Execute Command": { "message": ":STOP" }
          },
          "Single": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "SINGLE",
            "Execute Command": { "message": ":SINGle" }
          },
          "Auto_Scale": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "AUTO",
            "Execute Command": { "message": ":AUToscale" }
          }
        }
      },
      "Channel_Display_Control": {
        "type": "OcaBlock",
        "fields": {
          "CH1_ON": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "label_inactive": "CH1 ON",
            "Execute Command": { "message": ":CHANnel1:DISPlay ON" }
          },
          "CH1_OFF": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "label_inactive": "CH1 OFF",
            "Execute Command": { "message": ":CHANnel1:DISPlay OFF" }
          }
        }
      
      }
    }
  }
}