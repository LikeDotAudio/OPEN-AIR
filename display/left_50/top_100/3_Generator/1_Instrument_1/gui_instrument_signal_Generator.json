{
  "nab": {
    "type": "OcaBlock",
    "description": "SigGen Readbacks",
    "fields": {
      "Read_Current_Setup": {
        "type": "OcaBlock",
        "description": "Query active configuration",
        "fields": {
          "Get_Apply_String": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "APPLy?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "config_string": { "value": "\"SIN 1000, 3.0, 0.0\"", "type": "_Value" }
                }
              }
            }
          },
          "Get_Output_State": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "OUTPut?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "is_on": { "value": "0 or 1", "type": "_Value" }
                }
              }
            }
          },
          "Get_Error_Queue": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "SYSTem:ERRor?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "error_msg": { "value": "Error Code, Message", "type": "_Value" }
                }
              }
            }
          }
        }
      }
    }
  },
  "rig": {
    "type": "OcaBlock",
    "description": "Waveform Configuration",
    "fields": {
      "Quick_Setup_Apply": {
        "type": "OcaBlock",
        "description": "APPLy Macro Commands",
        "fields": {
          "Apply_Sine": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "APPLy:SINusoid <freq>, <amp>, <offset>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "freq": { "value": "1000", "type": "_Value" },
                  "amp": { "value": "1.0", "type": "_Value", "description": "Amplitude in Vpp" },
                  "offset": { "value": "0.0", "type": "_Value", "description": "Offset in Volts" }
                }
              }
            }
          },
          "Apply_Square": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "APPLy:SQUare <freq>, <amp>, <offset>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "freq": { "value": "1000", "type": "_Value" },
                  "amp": { "value": "1.0", "type": "_Value" },
                  "offset": { "value": "0.0", "type": "_Value" }
                }
              }
            }
          },
          "Apply_Pulse": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "APPLy:PULSe <freq>, <amp>, <offset>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "freq": { "value": "1000", "type": "_Value" },
                  "amp": { "value": "1.0", "type": "_Value" },
                  "offset": { "value": "0.0", "type": "_Value" }
                }
              }
            }
          }
        }
      },
      "Modulation_Setup": {
        "type": "OcaBlock",
        "description": "AM/FM/Pulse Config",
        "fields": {
          "Setup_AM": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "AM:INTernal:FUNCtion <shape>;AM:INTernal:FREQuency <freq>;AM:DEPTh <depth>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "shape": { "value": "SIN", "type": "_Value", "description": "SIN, SQU, TRI, RAMP" },
                  "freq": { "value": "100", "type": "_Value", "description": "Modulating Freq Hz" },
                  "depth": { "value": "50", "type": "_Value", "description": "Depth %" }
                }
              }
            }
          },
          "Setup_Pulse_Width": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "FUNCtion:PULSe:WIDTh <width_sec>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "width_sec": { "value": "0.0001", "type": "_Value" }
                }
              }
            }
          }
        }
      }
    }
  },
  "set": {
    "type": "OcaBlock",
    "description": "Individual Parameters",
    "fields": {
      "Set_Main_Parameters": {
        "type": "OcaBlock",
        "fields": {
          "Set_Frequency": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "FREQuency <freq>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "freq": { "value": "1000", "type": "_Value" } }
              }
            }
          },
          "Set_Amplitude": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "VOLTage <amp>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "amp": { "value": "1.0", "type": "_Value", "description": "Default unit is Vpp" } }
              }
            }
          },
          "Set_Offset": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "VOLTage:OFFSet <offset>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "offset": { "value": "0.0", "type": "_Value" } }
              }
            }
          },
          "Set_Function_Shape": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "FUNCtion <shape>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "shape": { "value": "SIN", "type": "_Value", "description": "SIN, SQU, RAMP, PULS, NOIS, DC, USER" }
                }
              }
            }
          }
        }
      },
      "Set_Units_Load": {
        "type": "OcaBlock",
        "fields": {
          "Set_Output_Load": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "OUTPut:LOAD <ohms>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "ohms": { "value": "50", "type": "_Value", "description": "Ohms or INFinity" } }
              }
            }
          },
          "Set_Voltage_Unit": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "VOLTage:UNIT <unit>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": { "unit": { "value": "VPP", "type": "_Value", "description": "VPP, VRMS, DBM" } }
              }
            }
          }
        }
      }
    }
  },
