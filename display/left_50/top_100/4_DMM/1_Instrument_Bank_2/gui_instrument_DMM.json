{
  "DMM_Panel": {
    "type": "OcaBlock",
    "description": "Digital Multimeter Control Panel",
    "fields": {
      "Measurement_Type": {
        "type": "OcaBlock",
        "description": "Measurement Type",
        "fields": {
          "measure_func": {
            "type": "_GuiDropDownOption",
            "label_active": "Function",
            "options": ["VOLT:DC", "VOLT:AC", "CURR:DC", "CURR:AC", "RES", "CONT", "DIOD"],
            "default_option": "VOLT:DC",
            "message_details": {
              "DMM_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "FUNC '<value>'"
              }
            }
          }
        }
      },
      "Read_Value": {
        "type": "OcaBlock",
        "description": "Read Measurement",
        "fields": {
          "read_button": {
            "type": "_GuiActuator",
            "description": "Read Now",
            "message_details": {
              "DMM_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "READ?"
              }
            }
          },
          "display_value": {
            "type": "_Value",

            "value": "---"
          }
        }
      }
    }
  }
}
