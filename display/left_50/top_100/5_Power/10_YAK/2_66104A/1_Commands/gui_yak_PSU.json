{
  "nab": {
    "type": "OcaBlock",
    "description": "MPS 8-Slot Metering",
    "fields": {
      "Measurements": {
        "type": "OcaBlock",
        "fields": {
          "Get_Values": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "INST:NSEL 1;MEAS:VOLT?;MEAS:CURR?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "volts": { "value": "V", "type": "_GuiValue", "label": "Voltage", "units": "V" },
                  "amps": { "value": "A", "type": "_GuiValue", "label": "Current", "units": "A" }
                }
              }
            }
          }
        }
      }
    }
  },
  "rig": {
    "type": "OcaBlock",
    "description": "Configuration (Volts/Amps)",
    "fields": {
      "Config_Slot": {
        "type": "OcaBlock",
        "fields": {
          "Set_S1_Levels": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "INST:NSEL 1;VOLT <volt>;CURR <curr>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "volt": { "value": "5.0", "type": "_GuiValue", "label": "Voltage", "units": "V" },
                  "curr": { "value": "1.0", "type": "_GuiValue", "label": "Current", "units": "A" }
                }
              }
            }
          }
        }
      }
    }
  },
  "Do": {
    "type": "OcaBlock",
    "description": "System Actions",
    "fields": {
      "Output_Toggles": {
        "type": "OcaBlock",
        "description": "Individual Output Control",
        "fields": {
          "Slot_1_Controls": {
            "type": "OcaBlock",
            "fields": {
              "S1_ON": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "label_inactive": "ON",
                "Execute Command": { "message": "INST:NSEL 1;OUTP ON" }
              },
              "S1_OFF": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "label_inactive": "OFF",
                "Execute Command": { "message": "INST:NSEL 1;OUTP OFF" }
              }
            }
          }
        }
      },
      
      "System_Control": {
        "type": "OcaBlock",
        "fields": {
          "System_Reset": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "Reset (*RST)",
            "Execute Command": { "message": "*RST" }
          },
          "Clear_Status": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "Clear (*CLS)",
            "Execute Command": { "message": "*CLS" }
          }
        }
      }
    }
  }
}