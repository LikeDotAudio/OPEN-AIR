              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "VOLT <value>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "value": { "value": "12.0", "type": "_GuiValue", "label": "Voltage", "units": "V", "description": "Volts" }
                }
              }
            }
          },
          "Set_Voltage_Range": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "VOLT:RANG <value>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "value": { "value": "60", "type": "_GuiValue", "label": "Range", "units": "V", "description": "6, 60, etc" }
                }
              }
            }
          }
        }
      },
      "Resistance_Settings": {
        "type": "OcaBlock",
        "description": "Constant Resistance Parameters",
        "fields": {
          "Set_Resistance_Level": {
            "type": "OcaBlock",
