{
  "nab": {
    "type": "OcaBlock",
    "description": "DC Load Measurements",
    "fields": {
      "Primary_Metering": {
        "type": "OcaBlock",
        "description": "Voltage, Current, Power",
        "fields": {
          "Measure_All": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "MEAS:VOLT?;MEAS:CURR?;MEAS:POW?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "volts": { "value": "V", "type": "_GuiValue", "label": "Voltage", "units": "V" },
                  "amps": { "value": "A", "type": "_GuiValue", "label": "Current", "units": "A" },
                  "watts": { "value": "W", "type": "_GuiValue", "label": "Power", "units": "W" }
                }
              }
            }
          }
        }
      },
      "System_Status": {
        "type": "OcaBlock",
        "description": "Diagnostics",
        "fields": {
          "Read_Status_Byte": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "*STB?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "status_byte": { "value": "Byte Value", "type": "_GuiValue", "label": "Status", "units": "" }
                }
              }
            }
          },
          "Read_Error": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "SYST:ERR?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "error_msg": { "value": "Error Message", "type": "_GuiValue", "label": "Error", "units": "" }
                }
              }
            }
          },
          "Read_IDN": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "*IDN?",
                "layout": { "height": 30, "sticky": "" }
              },
              "Outputs": {
                "type": "OcaBlock",
                "fields": {
                  "idn_string": { "value": "Device ID", "type": "_GuiValue", "label": "ID String", "units": "" }
                }
              }
            }
          }
        }
      }
    }
  },
  "rig": {
