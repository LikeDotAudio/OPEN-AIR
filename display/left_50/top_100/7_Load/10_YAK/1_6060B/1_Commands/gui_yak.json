                }
              }
            }
          },
          "Set_Resistance_Range": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": "RES:RANG <value>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "value": { "value": "1000", "type": "_GuiValue", "label": "Range", "units": "Ohms", "description": "Range" }
                }
              }
            }
          }
        }
      }
    }
  },
