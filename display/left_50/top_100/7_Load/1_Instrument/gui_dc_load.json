{
  "DC_Load_Station": {
    "type": "OcaBlock",
    "description": "Energy Dissipation Interface",
    "fields": {
      "Activation_Sector": {
        "type": "OcaBlock",
        "description": "MAIN INPUT CONTROL",
        "fields": {
          "Master_Input_Switch": {
            "type": "_TrapezoidButton",
            "label_active": "ðŸ”¥ LOAD ACTIVE (SINKING) ðŸ”¥",
            "label_inactive": "LOAD DISENGAGED",
            "state_default": false,
            "message_details": {
              "DC_LOAD_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "INP <state>"
              }
            }
          }
        }
      },
      "Telemetry_Deck": {
        "type": "OcaBlock",
        "description": "Real-Time Drain Analysis",
        "fields": {
          "Meter_Volts": {
            "type": "_GuiAnalogGauge",
            "label_active": "INPUT VOLTS",
            "min": "0",
            "max": "100",
            "units": "V",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "MEAS:VOLT?" } }
          },
          "Meter_Amps": {
            "type": "_GuiAnalogGauge",
            "label_active": "SINK CURRENT",
            "min": "0",
            "max": "60",
            "units": "A",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "MEAS:CURR?" } }
          },
          "Meter_Watts": {
            "type": "_GuiAnalogGauge",
            "label_active": "DISSIPATION",
            "min": "0",
            "max": "300",
            "units": "W",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "MEAS:POW?" } }
          }
        }
      },
      "History_Sector": {
        "type": "OcaBlock",
        "description": "Visual Log",
        "fields": {
          "Load_Graph": {
            "type": "_GuiGraph",
            "label_active": "Drain Timeline",
            "description": "Voltage/Current Drop over Time",
            "height": 200,
            "message_details": {
              "DC_LOAD_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "MEAS:ALL?"
              }
            }
          }
        }
      },
      "Mode_Logic_Sector": {
        "type": "OcaBlock",
        "description": "Physics Configuration",
        "fields": {
          "Mode_Selector": {
            "type": "_GuiDropDownOption",
            "label_active": "Operating Mode",
            "options": ["CC (Const Current)", "CV (Const Volt)", "CR (Const Res)", "CP (Const Power)"],
            "default_option": "CC (Const Current)",
            "message_details": {
              "DC_LOAD_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "MODE <value>"
              }
            }
          }
        }
      },
      "Control_Banks": {
        "type": "OcaBlock",
        "description": "Parameter Adjustment",
        "fields": {
          "CC_Controls": {
            "type": "OcaBlock",
            "description": "Constant Current (CC)",
            "fields": {
              "Set_Current": {
                "type": "_GuiHorizontalKnob",
                "label_active": "Current Set",
                "value_default": "0.0",
                "min": "0.0",
                "max": "60.0",
                "units": "A",
                "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "CURR <value>" } }
              },
              "Set_Slew": {
                "type": "_GuiHorizontalKnob",
                "label_active": "Slew Rate",
                "value_default": "1000",
                "min": "10",
                "max": "5000",
                "units": "A/s",
                "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "CURR:SLEW <value>" } }
              }
            }
          },
          "CV_CR_Controls": {
            "type": "OcaBlock",
            "description": "CV / CR Parameters",
            "fields": {
              "Set_Voltage": {
                "type": "_GuiHorizontalKnob",
                "label_active": "Voltage Set",
                "value_default": "12.0",
                "min": "0.1",
                "max": "100.0",
                "units": "V",
                "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "VOLT <value>" } }
              },
              "Set_Resistance": {
                "type": "_GuiHorizontalKnob",
                "label_active": "Resistance Set",
                "value_default": "100",
                "min": "0.1",
                "max": "10000",
                "units": "Î©",
                "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "RES <value>" } }
              }
            }
          }
        }
      },
      "Transient_Lab": {
        "type": "OcaBlock",
        "description": "Dynamic Pulse Testing",
        "fields": {
          "Trans_Enable": {
            "type": "_GuiButtonToggle",
            "label_active": "PULSE MODE ON",
            "label_inactive": "PULSE MODE OFF",
            "state_default": false,
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "TRAN:STAT <value>" } }
          },
          "Level_A": {
            "type": "_GuiHorizontalKnob",
            "label_active": "Level A (Base)",
            "value_default": "1.0",
            "units": "A",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "CURR:LEV <value>" } }
          },
          "Level_B": {
            "type": "_GuiHorizontalKnob",
            "label_active": "Level B (Peak)",
            "value_default": "5.0",
            "units": "A",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "CURR:TLEV <value>" } }
          },
          "Pulse_Freq": {
            "type": "_GuiHorizontalKnob",
            "label_active": "Frequency",
            "value_default": "100",
            "min": "0.1",
            "max": "10000",
            "units": "Hz",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "TRAN:FREQ <value>" } }
          },
          "Pulse_Duty": {
            "type": "_GuiHorizontalKnob",
            "label_active": "Duty Cycle",
            "value_default": "50",
            "min": "1",
            "max": "99",
            "units": "%",
            "message_details": { "DC_LOAD_MODEL": { "type": "_GuiActuator", "trigger": false, "command_value": "TRAN:DCYC <value>" } }
          }
        }
      }
    }
  }
}
