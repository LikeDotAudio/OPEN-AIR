        "type": "OcaBlock",
        "description": "PRIMARY CONTACTORS",
        "fields": {
          "Master_Switch": {
            "type": "_TrapezoidButton",
            "label_active": "ðŸ©¸ DRAINING ACTIVE ðŸ©¸",
