{
  "rig": {
    "type": "OcaBlock",
    "description": "Analog Signal Generator",
    "fields": {
      "Generator_Waveforms": {
        "type": "OcaBlock",
        "description": "Select Waveform Shape",
        "fields": {
          "Set_Wave_Sine": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":WAVeform SINE",
                "layout": { "height": 30, "sticky": "" }
              }
            }
          },
          "Set_Wave_Square": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":WAVeform SQUare",
                "layout": { "height": 30, "sticky": "" }
              }
            }
          },
          "Set_Wave_IMD": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":WAVeform IMD",
                "layout": { "height": 30, "sticky": "" }
              }
            }
          }
        }
      },
      "Generator_Parameters": {
        "type": "OcaBlock",
        "description": "Set Signal Parameters",
        "fields": {
          "Set_Amplitude": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":GAMPlitude <amp>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "amp": { "value": "1.0 V", "type": "_Value",
 "description": "e.g., 1.0 V, 0 DBV" }
                }
              }
            }
          },
          "Set_Frequency": {
            "type": "OcaBlock",
            "fields": {
              "Execute Command": {
                "type": "_GuiActuator",
                "AES70": "OcaBooleanActuator",
                "active": true,
                "message": ":GFREquency <freq>",
                "layout": { "height": 30, "sticky": "" }
              },
              "Input": {
                "type": "OcaBlock",
                "fields": {
                  "freq": { "value": "1000 HZ", "type": "_Value",
 "description": "Frequency in Hz" }
                }
              }
            }
          }
        }
      }
    }
  }
}